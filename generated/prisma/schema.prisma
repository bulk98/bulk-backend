generator client {
  provider = "prisma-client-js"
  output   = "../generated/prisma"
}

datasource db {
  provider = "mongodb"
  url      = "mongodb://localhost:27017/zumbradb"
}

model User {
  id           String @id @default(auto()) @map("_id") @db.ObjectId
  email        String @unique
  password     String
  tipo_usuario String // Podr√≠a ser 'GURU' o 'MEMBER'
}
